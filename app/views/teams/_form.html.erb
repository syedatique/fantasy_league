<%= form_for @team do |f|%>

<%= f.label :name, 'full name'%>
<%= f.text_field :name%> 
<br>

<%= f.label :image%>
<%= f.text_field :image%> 
<br>

<%= f.label :founded%>
<%= f.date_select :founded%> 
<br>

<%= f.label :mascot%>
<%= f.text_field :mascot%> 
<br>

<%= f.label :league%>
<%= f.text_field :league%> 
<br>

<%= f.label :last_position%>
<%= f.text_field :last_position%> 
<br>
<%= f.submit button_text %>

<% end %>